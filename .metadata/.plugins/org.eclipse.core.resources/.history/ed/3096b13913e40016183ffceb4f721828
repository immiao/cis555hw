package edu.upenn.cis455.webserver.utils;

public class HttpResponseThread extends Thread{
	
	private TaskBlockingQueue queue;
	
	public void HttpResponseThread(TaskBlockingQueue queue) {
		this.queue = queue;
	}
	public void run() {
		while (true) {
			synchronized (queue) {
				while (!queue.empty()) {
					
				}
			}
		}
	}
}
